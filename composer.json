{
    "name": "andreg/spaces-in-parentheses-php-cs-fixer",
    "description": "A custom PHP CS Fixer to add or remove spaces in parentheses",
    "type": "library",
    "homepage": "https://github.com/SuperDJ/spaces-in-parentheses-php-cs-fixer",
    "readme": "./README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "SuperDJ",
            "email": "super.derkjan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "friendsofphp/php-cs-fixer": "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SuperDJ\\SpacesInParenthesesFixer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperDJ\\SpacesInParenthesesFixer\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
